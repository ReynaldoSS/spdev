{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "hello-world-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/helloWorld/HelloWorldWebPart.js",
          "manifest": "./src/webparts/helloWorld/HelloWorldWebPart.manifest.json"
        }
      ]
    },
    "new-hello-world-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/newHelloWorld/NewHelloWorldWebPart.js",
          "manifest": "./src/webparts/newHelloWorld/NewHelloWorldWebPart.manifest.json"
        }
      ]
    },
    "react-function-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reactFunction/ReactFunctionWebPart.js",
          "manifest": "./src/webparts/reactFunction/ReactFunctionWebPart.manifest.json"
        }
      ]
    },
    "react-query-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reactQuery/ReactQueryWebPart.js",
          "manifest": "./src/webparts/reactQuery/ReactQueryWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "HelloWorldWebPartStrings": "lib/webparts/helloWorld/loc/{locale}.js",
    "NewHelloWorldWebPartStrings": "lib/webparts/newHelloWorld/loc/{locale}.js",
    "ReactFunctionWebPartStrings": "lib/webparts/reactFunction/loc/{locale}.js",
    "ReactQueryWebPartStrings": "lib/webparts/reactQuery/loc/{locale}.js"
  }
}
